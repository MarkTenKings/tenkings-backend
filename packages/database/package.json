{
  "name": "@tenkings/database",
  "version": "0.1.0",
  "private": true,
  "main": "dist/database/src/index.js",
  "types": "dist/database/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/database/src/index.d.ts",
      "default": "./dist/database/src/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p .",
    "generate": "prisma generate --schema prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "db:push": "prisma db push --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tenkings/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "prisma": "^5.22.0",
    "typescript": "^5.5.4"
  }
}
