{
  "name": "tenkings-monorepo",
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": "20.x"
  },
  "private": true,
  "workspaces": [
    "backend/*",
    "frontend/*",
    "packages/*"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "lint": "echo \"(placeholder) configure ESLint\"",
    "format": "echo \"(placeholder) configure Prettier\"",
    "db:generate": "npm run generate --workspace @tenkings/database",
    "db:migrate": "npm run migrate:dev --workspace @tenkings/database",
    "db:deploy": "npm run migrate:deploy --workspace @tenkings/database",
    "db:push": "npm run db:push --workspace @tenkings/database",
    "variants:sync": "node scripts/variant-db/sync-variant-db.js",
    "variants:sports:discover": "node scripts/variant-db/discover-sports-sets.js",
    "variants:sports:build-csv": "node scripts/variant-db/build-sports-variants-csv.js",
    "variants:sports:build-checklist-sources": "node scripts/variant-db/build-checklist-source-manifest.js",
    "variants:sports:collect": "node scripts/variant-db/collect-sports-variants.js",
    "variants:sports:parse-checklist": "node scripts/variant-db/parse-checklist-players.js",
    "variants:sports:parse-checklists-batch": "node scripts/variant-db/parse-checklists-batch.js",
    "variants:sports:validate-checklist": "node scripts/variant-db/validate-checklist-output.js",
    "variants:sports:seed-refs": "node scripts/variant-db/seed-sports-reference-images.js",
    "variants:sports:build-player-map": "node scripts/variant-db/build-checklist-player-map.js",
    "variants:sports:qa-queue": "node scripts/variant-db/build-qa-gap-queue.js",
    "variants:sports:backfill-ref-quality": "node scripts/variant-db/backfill-reference-quality-gate.js",
    "variants:sports:coverage-gaps": "node scripts/variant-db/list-reference-coverage-gaps.js",
    "variants:sports:run": "node scripts/variant-db/run-sports-live.js",
    "vercel:build": "bash scripts/vercel-build.sh",
    "packs:mint": "node scripts/mint-pack-inventory.js"
  }
}
