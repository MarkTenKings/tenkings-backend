{
  "name": "tenkings-monorepo",
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": "20.x"
  },
  "private": true,
  "workspaces": [
    "backend/*",
    "frontend/*",
    "packages/*"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "lint": "echo \"(placeholder) configure ESLint\"",
    "format": "echo \"(placeholder) configure Prettier\"",
    "db:generate": "npm run generate --workspace @tenkings/database",
    "db:migrate": "npm run migrate:dev --workspace @tenkings/database",
    "db:deploy": "npm run migrate:deploy --workspace @tenkings/database",
    "db:push": "npm run db:push --workspace @tenkings/database",
    "vercel:build": "bash scripts/vercel-build.sh",
    "packs:mint": "node scripts/mint-pack-inventory.js"
  }
}
